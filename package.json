{
  "name": "@ldflex/comunica",
  "version": "3.4.2",
  "description": "Comunica query engine support for the LDflex language",
  "author": "Ruben Verborgh <ruben@verborgh.org> (https://ruben.verborgh.org/)",
  "contributors": [
    "Joachim Van Herwegen"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LDflex/LDflex-Comunica"
  },
  "lsd:module": true,
  "main": "lib/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "lib"
  ],
  "peerDependencies": {
    "ldflex": "^2.15.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@comunica/query-sparql": "^3.0.1",
    "@comunica/query-sparql-file": "^3.0.1",
    "@comunica/query-sparql-rdfjs": "^3.0.1",
    "@comunica/types": "^3.0.1",
    "@pollyjs/adapter-node-http": "^6.0.6",
    "@pollyjs/core": "^6.0.6",
    "@pollyjs/persister-fs": "^6.0.6",
    "@rdfjs/types": "^1.1.0",
    "@types/jest": "^29.5.12",
    "@types/n3": "^1.16.4",
    "@types/setup-polly-jest": "^0.5.5",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^27.9.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "ldflex": "^2.15.2",
    "mkdirp": "^3.0.1",
    "n3": "^1.17.3",
    "semantic-release": "^24.0.0",
    "setup-polly-jest": "^0.11.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.3"
  },
  "scripts": {
    "build": "tsc && babel dist --out-dir lib",
    "jest": "jest",
    "lint": "eslint src test --ext .ts",
    "lint:fix": "eslint src test --ext .ts --fix",
    "prepublishOnly": "npm run build",
    "test": "npm run lint && npm run jest",
    "test:dev": "npm run jest -- --watch"
  },
  "release": {
    "branches": [
      "master",
      "+([0-9])?(.{+([0-9]),x}).x",
      "next",
      {
        "name": "alpha",
        "prerelease": true
      },
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@comunica/query-sparql-solid": "^3.0.1"
  }
}
