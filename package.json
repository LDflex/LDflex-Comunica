{
  "name": "@ldflex/comunica",
  "version": "3.4.2",
  "description": "Comunica query engine support for the LDflex language",
  "author": "Ruben Verborgh <ruben@verborgh.org> (https://ruben.verborgh.org/)",
  "contributors": [
    "Joachim Van Herwegen"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LDflex/LDflex-Comunica"
  },
  "lsd:module": true,
  "main": "lib/index.js",
  "module": "src/index.js",
  "sideEffects": false,
  "files": [
    "src",
    "lib"
  ],
  "peerDependencies": {
    "ldflex": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@comunica/actor-http-proxy": "^1.22.1",
    "@comunica/actor-init-sparql-file": "^1.22.3",
    "@comunica/actor-init-sparql-rdfjs": "^1.22.3",
    "@pollyjs/adapter-node-http": "^6.0.4",
    "@pollyjs/core": "^6.0.4",
    "@pollyjs/persister-fs": "^6.0.4",
    "@rdfjs/data-model": "^1.3.4",
    "eslint": "^8.5.0",
    "eslint-plugin-jest": "^25.3.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "ldflex": "^2.13.0",
    "mkdirp": "^1.0.4",
    "n3": "^1.12.2",
    "semantic-release": "^18.0.1",
    "setup-polly-jest": "^0.10.0"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "jest": "jest",
    "lint": "eslint src test",
    "lint:fix": "eslint src test --fix",
    "prepublishOnly": "npm run build",
    "test": "npm run lint && npm run jest",
    "test:dev": "npm run jest -- --watch"
  },
  "release": {
    "branches": [
      "master",
      "+([0-9])?(.{+([0-9]),x}).x",
      "next",
      {
        "name": "alpha",
        "prerelease": true
      },
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@comunica/actor-init-sparql-solid": "^1.0.1"
  }
}
